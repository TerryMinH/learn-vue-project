{
    "version": "2.0",
    "envId": "xpf001-8gaixw4t74f34ccb",
    "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
    "framework": {
      "name": "cfsw",
      "hooks": {
        "preDeploy": {
          "type": "execCommand",
          "commands": [
            "yarn --registry=http://registry.npm.taobao.org",
            "yarn build",
            "yarn create:ssr"
          ]
        }
      },
      "plugins": {
        "client": {
          "use": "@cloudbase/framework-plugin-website",
          "inputs": {
            "installCommand": "tcb hosting delete && tcb hosting list",
            "buildCommand": "npm run build:static",
            "outputPath": "/dist",
            "cloudPath": "/",
            "envVariables": {
              "VUE_APP_ENV_ID": "{{env.ENV_ID}}"
            }
          }
        },
        "server": {
          "use": "@cloudbase/framework-plugin-function",
          "inputs": {
            "functionRootPath": "./cloudfunctions",
            "publishIncludeList": "{{env.publishIncludeList}}",
            "functions": [
              {
                "name": "ssr"
              }
            ],
            "servicePaths": {
              "cfsw": "/"
            }
          }
        }
      }
    }
  }
  